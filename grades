M1 = float(input('Nota 1: '))
M2 = float(input('Nota 2: '))
M3 = float(input('Nota 3: '))

media = (M1+M2+M3)/3

if media >= 0.0 and media<= 4.0:
    print('Reprovado')
elif media >= 4.1 and media <= 6.0:
    print('Pegou exame')
    print('A nota foi: ', media)
    print('Reprovado')
elif media >= 6.0: 
    print('Aprovado')
