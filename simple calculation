code1, number1, value_unit1 = input().split()
code2, number2, value_unit2 = input().split()

code1 = int(code1)
number1 = int(number1)
value_unit1 = float(value_unit1)

code2 = int(code2)
number2 = int(number2)
value_unit2 = float(value_unit2)

value_1 = number1 * value_unit1
value_2 = number2 * value_unit2

total_value = value_1 + value_2

print("VALOR A PAGAR: R$ %0.2f" %total_value)
